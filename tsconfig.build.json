{
  "compilerOptions": {
    "declaration": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015"],
    "skipLibCheck": true,
    "types": [],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "resolveJsonModule": true
  }
}
